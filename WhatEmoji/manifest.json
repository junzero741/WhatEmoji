{
  "name": "WhatEmoji",
  "description": "get emoji for every word",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker" : "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["data/emojiList.json"],
    "matches": ["<all_urls>"]
    }],
  "permissions": ["storage", "activeTab", "scripting"],
  "action" : {
    "default_popup" : "popup.html",
    "default_icon": {
      "16": "/images/think16.png",
      "32": "/images/think32.png",
      "48": "/images/think48.png",
      "128": "/images/think128.png"
    }
  },
  "icons": {
    "16": "/images/think16.png",
    "32": "/images/think32.png",
    "48": "/images/think48.png",
    "128": "/images/think128.png"
  }
}